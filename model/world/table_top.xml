        <geom name="box1" size="0.05 0.05 0.05" rgba="0.43 0.1647 0.1647 1" type="box" conaffinity="1"/>
      </body>
      <body name="table" pos="1.5 0 0.35" quat="0.707107 0 0 0.707107">
